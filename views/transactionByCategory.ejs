<h1>Transactions for <%= user.username %></h1>
<div>
  <p>Here is your list of transactions!</p>
  <ul>
    <li><a href="/transaction">transaction table</a></li>
  </ul>
</div>
<table class="table table-bordered table-striped" >
  <thead>
    <tr>
      <th>category</th>
      <th>amount</th>
    </tr>
  </thead>
  <tbody>
    <% results.map(item => {%>
    <tr>
      <td><%= item._id %></td>
      <td><%= item.total %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
